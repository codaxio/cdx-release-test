#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/m/workspaces/smardex/Backend-RA2-Tech/cdx-release-test/node_modules/.pnpm/@codaxio+cdx@0.26.0/node_modules/@codaxio/cdx/node_modules:/home/m/workspaces/smardex/Backend-RA2-Tech/cdx-release-test/node_modules/.pnpm/@codaxio+cdx@0.26.0/node_modules/@codaxio/node_modules:/home/m/workspaces/smardex/Backend-RA2-Tech/cdx-release-test/node_modules/.pnpm/@codaxio+cdx@0.26.0/node_modules:/home/m/workspaces/smardex/Backend-RA2-Tech/cdx-release-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/m/workspaces/smardex/Backend-RA2-Tech/cdx-release-test/node_modules/.pnpm/@codaxio+cdx@0.26.0/node_modules/@codaxio/cdx/node_modules:/home/m/workspaces/smardex/Backend-RA2-Tech/cdx-release-test/node_modules/.pnpm/@codaxio+cdx@0.26.0/node_modules/@codaxio/node_modules:/home/m/workspaces/smardex/Backend-RA2-Tech/cdx-release-test/node_modules/.pnpm/@codaxio+cdx@0.26.0/node_modules:/home/m/workspaces/smardex/Backend-RA2-Tech/cdx-release-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/bash" ]; then
  exec "$basedir/bash"  "$basedir/../@codaxio/cdx/start.sh" "$@"
else
  exec bash  "$basedir/../@codaxio/cdx/start.sh" "$@"
fi
